  "version": "0.2.0",
  "configurations": [
    {
      "name": "tap-simplesat discovery",
      "type": "python",
      "request": "launch",
      "module": "tap_simplesat.tap",
      "args": ["--config", ".secrets/config.json", "--discover"],
      "python": "${command:python.interpreterPath}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
        "name": "tap-simplesat launch",
        "type": "python",
        "request": "launch",
        "module": "tap_simplesat.tap",
        "args": ["--config", ".secrets/config.json"],
        "python": "${command:python.interpreterPath}",
        "console": "integratedTerminal",
        "justMyCode": true
      }
  ]
}
